<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo">
    <ActionBar title="Select Category" />
    <StackLayout>
        <Label text="Select a Category:" />
        <ListPicker id="categoryPicker" items="{{ items }}" selectedIndexChange="onSelectedIndexChange" />
        <Button text="Next" tap="onNextTap" />
    </StackLayout>
</Page>